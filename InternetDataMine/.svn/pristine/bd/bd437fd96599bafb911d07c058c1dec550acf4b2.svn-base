<TreeList>
  <Properties  Title="实时数据" editAble="false" bgColor="#FFFFFF" headerBgColor="#FFFFFF" curSelBackColor="#d7f4fc" fadeInStep="0" rowHeight="25" GridColor="#b4c3c8"  HeaderFontIndex="2" addRowAble="false" deleteRowAble="false" ></Properties>
  <Fonts>
    <Font faceName="Consolas"/>
    <Font />
    <Font faceName="微软雅黑" bold="1"/>
    <Font bold="true" />
  </Fonts>
  <Cols>
    <Col name="TmpID"  isHide="absTrue" width="5%" align="center">ID号</Col>
    <Col name="SimpleName"  width="15%" align="center"  isHide="absTrue">煤矿名称</Col>
    <Col name="SensorNum"  width="12%" align="center">测点编号</Col>
    <Col name="TypeName"  width="12%" align="center">设备名</Col>
    <Col name="Place"  width="22%" align="center">安装地点</Col>
    <Col name="ShowValue"   width="10%" align="center"  displayMask="textColor=if(ValueState='正常'||ValueState='复电',green,if(ValueState='报警',red,if(ValueState='故障',magenta,if(ValueState='馈电异常',darkorange,if(ValueState='工作异常',maroon,if(ValueState='分站故障',orange,red))))))">测点值</Col>
    <Col name="unit"  width="7%" align="center">单位</Col>
    <Col name="datetime" datatype="datetime"   width="27%" align="center">监测时间</Col>
    <Col name="ValueState"   width="10%" align="center" displayMask="textColor=if(data='正常'||data='复电',green,if(data='报警',red,if(data='故障',magenta,if(data='馈电异常',darkorange,if(data='工作异常',maroon,if(data='分站故障',orange,red)))))) ">状态</Col>
  </Cols>
</TreeList>