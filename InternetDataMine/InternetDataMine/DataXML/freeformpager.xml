<?xml version="1.0" encoding="utf-8" ?>

<freeform>
  <Properties bgColor="#ffffff"/>
  <Fonts>
    <Font facename="consolas" italic="1" />
  </Fonts>
  <Objects>
    <!-- 背景图和轮廓线，使用了下面的圆角矩形路径，注意：y1=" -100%"表示上半部分是不显示的 -->
    <Img  x1="0" x2="100%" y1="-100%" y2="100%" pathID="path1" src="../images/pagerback.png" arrange="stretch"/>
    <Rect x1="0" x2="100%" y1="-100%" y2="100%" pathID="path1" pathBorder="thick=1;color=#377d8d" thick="0"/>
    <!-- 分页器 -->
    <pager id="ID0" x1="100" y="0" x2="99%" height="100%" textColor="#0799B7" PageRowsList="10,15,20,100,1000" pageRows="100" />
    <!-- 看不见的线条，没什么用处，仅仅用于表示freeform的高度（38） -->
    <line x="10" y="0" width="0" height="38" thick="0" />
  </Objects>
  <paths>
    <path id="path1">
      <!-- 圆角矩形，圆角直径:30 -->
      <Rect x="0" y="0" width="100%" height="100%" round="30"/>
    </path>
  </paths>
</freeform>